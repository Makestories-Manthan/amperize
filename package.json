    "lodash": "^4.17.11",
    "request": "^2.83.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "mocha": "^7.0.0",
    "nock": "^12.0.0",
    "nyc": "^15.0.0",
    "rewire": "^5.0.0",
