  "author": "Manthan Mehta <manthan.mehta@techno-soft.in>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Makestories-Manthan/amperize/issues"
  },
  "homepage": "https://github.com/Makestories-Manthan/amperize#readme",
  "dependencies": {
    "async": "^3.0.1",
    "domutils": "^2.0.0",
    "emits": "^3.0.0",
